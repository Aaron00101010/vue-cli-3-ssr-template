{
  //安装eslint,stylelint,prettier插件
  "editor.formatOnSave": false,
  "[scss]":{
    "editor.formatOnSave": true
  },
  "[css]":{
    "editor.formatOnSave": true
  },
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
}
